	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>google-signin</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.371878262982-msdt2oq5rl858ft64d33onhrg5l67ofu</string>
			</array>
		</dict>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>app-callback</string>
			<key>CFBundleURLSchemes</key>
			<array>
			<string>com.darrylgraham.revolution</string>
		</array>
	</dict>
	<dict>
		<key>CFBundleTypeRole</key>
		<string>Editor</string>
		<key>CFBundleURLName</key>
		<string>widget-deeplink</string>
		<key>CFBundleURLSchemes</key>
		<array>
			<string>cosmiq</string>
		</array>
	</dict>
	</array>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSCameraUsageDescription</key>
	<string>We use the camera only if you choose to take photos for your profile, progress tracking, or to attach images to your quests.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>We use the microphone for voice input to create tasks and chat with your mentor using your voice.</string>
	<key>NSSpeechRecognitionUsageDescription</key>
	<string>We use speech recognition to convert your voice into text for creating tasks and sending messages.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>We access your photo library only if you choose to attach images to your quests or update your profile photo.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>We save photos to your library only if you choose to download images from the app.</string>
	<key>NSContactsUsageDescription</key>
	<string>We use your contacts to help you quickly add people to your network without typing their information manually.</string>
	<key>NSCalendarsUsageDescription</key>
	<string>We use calendar access to let you optionally send quests to your Apple Calendar and keep linked events in sync.</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
